    "@astrojs/starlight-tailwind": "^4.0.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@tailwindcss/vite": "^4.1.14",
    "astro": "^5.14.1",
    "astro-diagram": "^0.7.0",
    "hast-util-select": "^6.0.4",
    "hastscript": "^9.0.1",
    "marked": "^16.3.0",
    "rehype-mermaid": "3.0.0",
    "remark-svgbob": "^2.0.0",
    "sharp": "^0.34.4",
    "starlight-links-validator": "0.18.0",
